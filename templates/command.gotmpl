{{- define "command" -}}
package main

import (
	"fmt"

	"{{.ServiceImportPath}}"
	"github.com/nanozuki/akko"
)

func main() {
	{{.ServicePkg}}.AkkoService()
    akko.ParseDirectory("{{.PkgDir}}")
	fmt.Println("Generated Akko, services are:")
	for _, rg := range akko.RouteGroups {
		fmt.Printf("%v\n", rg)
	}
}
{{- end -}}
